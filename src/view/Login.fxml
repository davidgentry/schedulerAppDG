<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginController">
   <children>
      <AnchorPane layoutX="26.0" layoutY="53.0" prefHeight="323.0" prefWidth="560.0">
         <children>
            <Button fx:id="loginButton" layoutX="313.0" layoutY="247.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="34.0" prefWidth="108.0" />
            <HBox layoutX="119.0" layoutY="141.0" prefHeight="43.0" prefWidth="303.0">
               <children>
                  <Label fx:id="pwdLabel" prefHeight="17.0" prefWidth="106.0" />
                  <PasswordField fx:id="passwordField" prefHeight="25.0" prefWidth="223.0" />
               </children>
            </HBox>
            <HBox layoutX="119.0" layoutY="79.0" prefHeight="34.0" prefWidth="303.0">
               <children>
                  <Label fx:id="userLabel" prefHeight="17.0" prefWidth="90.0" />
                  <TextField fx:id="usernameField" prefHeight="25.0" prefWidth="214.0" />
               </children>
            </HBox>
            <Label fx:id="messageLabel" layoutX="119.0" layoutY="178.0" prefHeight="25.0" prefWidth="303.0" textFill="#ff0404" />
            <Label fx:id="errorLabel" layoutX="119.0" layoutY="203.0" prefHeight="25.0" prefWidth="303.0" textFill="#ff0404" />
            <Label fx:id="title" layoutX="75.0" prefHeight="34.0" prefWidth="266.0">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
